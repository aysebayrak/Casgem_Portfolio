@model Casgem_Portfolio.Models.Entities.TblProject
@{
    ViewBag.Title = "UpdateProject";
    Layout = "~/Views/Shared/_PortfolioAdminLayout.cshtml";
}

<h2>Proje Güncelle</h2>

<form method="post">
    @Html.HiddenFor(x => x.ProjectID)
    <label>Proje Adı</label>
    @Html.TextBoxFor(x => x.ProjectName, new { @class = "form-control" })
    <br />
    <label>Proje Konusu</label>
    @Html.TextBoxFor(x => x.ProjectTopic,  new { @class = "form-control" })
  
    <br />
    <button class="btn btn-success"> Güncelle</button>
</form>

